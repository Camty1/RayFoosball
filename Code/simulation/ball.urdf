<robot name="ball">
    <link name="base_link">
        <visual>
            <origin rpy="0 0 0" xyx="0 0 0"/>
            <geometry>
                <sphere radius=".0175"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <sphere radius=".0175"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0"/>
        </collision>
        <contact>
            <lateral_friction value="0.5"/>
            <rolling_friction value="0.0"/>
            <contact_cfm value="0.0"/>
            <contact_erp value="1.0"/>
        </contact>
        <inertial>
            <mass value=".01"/>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <inertia ixx=".000001225" ixy="0" ixz="0" iyy=".000001225" iyz="0" izz=".000001225"/>
        </inertial>
    </link>
</robot>

